files:
    /etc/nginx/conf.d/proxy.conf:
        content: |
            client_max_body_size 10M; 
            upstream nodejs {
                server 127.0.0.1:5000;
                keepalive 256;
            }
            server {
                listen 8080;
                location / {
                    proxy_read_timeout 300s;
                    proxy_connect_timeout 75s;
                }
            }
      
